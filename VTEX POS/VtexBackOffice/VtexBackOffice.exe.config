    <add name="OracleCNQa" connectionString="Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=gwventastest)(PORT=1888))(CONNECT_DATA=(SERVICE_NAME=GWV)));User Id=intapp;Password=integra2018;" providerName="Oracle.ManagedDataAccess.Client" />
